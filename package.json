{
	"name": "iamturns-scripts",
	"version": "0.0.0-development",
	"description": "Common scripts for my projects",
	"bin": {
		"iamturns-scripts": "src/index.js"
	},
	"scripts": {
		"reinstall": "node src/index.js reinstall",
		"upgrade": "node src/index.js upgrade",
		"upgrade-latest": "node src/index.js upgrade-latest",
		"reset": "node src/index.js reset",
		"format": "kcd-scripts format",
		"lint": "kcd-scripts lint",
		"validate": "kcd-scripts validate",
		"setup": "npm install && npm run validate -s",
		"precommit": "kcd-scripts precommit",
		"semantic-release": "semantic-release"
	},
	"dependencies": {
		"cross-spawn": "^6.0.5",
		"debug": "^3.1.0",
